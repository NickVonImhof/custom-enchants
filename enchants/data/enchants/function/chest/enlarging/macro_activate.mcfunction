$attribute @s minecraft:generic.scale modifier add enlarging $(scale) add_value
$attribute @s minecraft:generic.step_height modifier add enlarging $(step) add_value
$attribute @s minecraft:generic.movement_speed modifier add enlarging $(speed) add_value
$attribute @s minecraft:generic.jump_strength modifier add enlarging $(jump) add_value
$attribute @s minecraft:generic.safe_fall_distance modifier add enlarging $(fall) add_value
$attribute @s minecraft:generic.attack_knockback modifier add enlarging $(knockback) add_value
$attribute @s minecraft:player.block_interaction_range modifier add enlarging $(blockinter) add_value
$attribute @s minecraft:player.entity_interaction_range modifier add enlarging $(entityinter) add_value

$particle explosion ~ ~$(particle_height) ~ $(particle_delta) $(particle_delta) $(particle_delta) 0.05 $(particle_count) normal
